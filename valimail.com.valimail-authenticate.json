{
  "providerId": "valimail.com",
  "providerName": "Valimail",
  "serviceId": "valimail-authenticate",
  "serviceName": "Valimail Authenticate",
  "version": 1,
  "logoUrl": "https://docs.valimail.com/css/image/portal-logo.png",
  "description": "Enable Valimail Authenticate on a domain",
  "variableDescription": "No extra variables are used, %domain% is required and built in to the protocol",
  "records": [
      {
      "type": "NS",
      "host": "_dmarc",
      "pointsTo": "ns.vali.email",
      "ttl": 300
    },
    {
      "type": "NS",
      "host": "_domainkey",
      "pointsTo": "ns.vali.email",
      "ttl": 300
    },
    {
      "type": "NS",
      "host": "_bimi",
      "pointsTo": "ns.vali.email",
      "ttl": 300
    },
    {
      "type": "TXT",
      "host": "@",
      "data": "v=spf1 include:%domain%._nspf.vali.email include:%{i}._ip.%{h}._ehlo.%{d}._spf.vali.email ~all",
      "ttl": 300,
      "txtConflictMatchingMode": "Prefix",
      "txtConflictMatchingPrefix": "v=spf1"
    }
  ]
}
